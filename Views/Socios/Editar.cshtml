@model Socios
@{
    ViewData["Title"] = "Home Page"; 
}

<div class="text-center">
    <h1 class="display-4">Edicion de Socio</h1>
    @Html.ActionLink("Volver","Index")

@using (@Html.BeginForm())
{
<div class="form-group">
    @Html.LabelFor(m=> m.NroSocio)
    @Html.TextAreaFor(m=> m.NroSocio,new {@class="form-control",@readonly = "readonly"} )    
</div>
<div class="form-group">
    @Html.LabelFor(m=> m.Nombre)
    @Html.TextAreaFor(m=> m.Nombre, new {@class="form-control"} )
</div>
<div class="form-group">
    @Html.LabelFor(m=> m.Apellido)
    @Html.TextAreaFor(m=> m.Apellido, new {@class="form-control"})
</div>
<div class="form-group">
    @Html.LabelFor(m=>m.fecha_nac);
    @Html.TextAreaFor(m=>m.fecha_nac, new {@class="form-control"});
</div>

    <input type="submit" class="btn btn-primary" value="Guardar Alumno" />
    
}
 

</div>
