@model List<Socios>
@{
    ViewData["Title"] = "Gymnasio Olimpia";
    var mensaje=ViewBag.mensaje;
}

<div class="text-center">
    <h1 class="display-4">Estado Socios Atrasados</h1>
    <h1>@mensaje</h1>
    @Html.ActionLink("Volver","Index")

    <table class="table">
        <tr>
            <th>Nro. Socio</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Fecha Pago</th>
            <th>Estado</th>
            <th>Matricula</th>
            <th></th>
        </tr>

    @foreach (var Socios in Model)
    {
        <tr>
                <td>@Socios.NroSocio</td>
                <td>@Socios.Nombre</td>
                <td>@Socios.Apellido</td>
                <td>@Socios.fecha_pago.ToString("dd/MM/yy")</td>
                <td>@Socios.estado</td>
                <td>@Socios.Matricula</td>
                
        </tr>
    }
    </table>


</div>
